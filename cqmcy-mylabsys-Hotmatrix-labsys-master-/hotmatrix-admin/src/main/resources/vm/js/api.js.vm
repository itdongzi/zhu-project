import request from '@/myutil/request'

// 查询${functionName}列表
export function list${BusinessName}(data) {
  return request({
    url: '/${moduleName}/${businessName}/list',
    method: 'post',
    data: data
  })
}

// 查询${functionName}详细
export function get${BusinessName}(id) {
  return request({
    url: '/${moduleName}/${businessName}/' + id,
    method: 'get'
  })
}

// 新增${functionName}
export function add${BusinessName}(data) {
  return request({
    url: '/${moduleName}/${businessName}',
    method: 'post',
    data: data
  })
}

// 修改${functionName}
export function upt${BusinessName}(data) {
  return request({
    url: '/${moduleName}/${businessName}',
    method: 'put',
    data: data
  })
}

// 删除${functionName}
export function del${BusinessName}(ids) {
  return request({
    url: '/${moduleName}/${businessName}/' + ids,
    method: 'delete'
  })
}

// 导出${functionName}
export function expt${BusinessName}(data) {
  return request({
    url: '/${moduleName}/${businessName}/export',
